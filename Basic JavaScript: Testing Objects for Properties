// Setup
var myObj = {
  gift: "pony",
  pet: "kitten",
  bed: "sleigh"
};

function checkObj(checkProp) {  //function checkObj drags the argument "gift" from the func call in line 22 to the parameter checkProp
  
  if (myObj.hasOwnProperty(checkProp) == true) {  // this looks up the var myObj.hasOwnProperty method(value = "gift") to see if it's true or false
    return myObj[checkProp];    //why does return myObj.checkProp not work?  Why does it need to be in brackets?
  }
  else {
      return "Not Found"
  }

  
  
}

// Test your code by modifying these values
checkObj("gift");


//Function parameters are the names listed in the function definition.
//Function arguments are the real VALUES passed to (and received by) the function.
